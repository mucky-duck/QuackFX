<?xml version="1.0" encoding="utf-8"?>
<!--QuackFX-->
<includes>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label/>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>GeneralTextColor</textcolor>
        <focusedcolor>GeneralTextColor</focusedcolor>
        <selectedcolor>GeneralTextColor</selectedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <scroll>false</scroll>
        <scrollspeed>30</scrollspeed>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <label/>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>GeneralTextColor</textcolor>
        <focusedcolor>GeneralTextColor</focusedcolor>
        <selectedcolor>GeneralTextColor</selectedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <scrollspeed>30</scrollspeed>
        <pauseatend>2000</pauseatend>
        <scrollout>false</scrollout>
    </default>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width/>
        <height/>
        <align>center</align>
        <aligny>center</aligny>
        <texture/>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor fallback="white">ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
        <textoffsetx>50</textoffsetx>
    </default>
	
	<default type="edit">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <pulseonselect>false</pulseonselect>
		<texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <textoffsetx>50</textoffsetx>
		<textoffsety>8</textoffsety>
    </default>
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <label/>
        <font>Light28</font>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <align>center</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
        <alttexturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</alttexturefocus>
        <alttexturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</alttexturenofocus>
        <textoffsetx>50</textoffsetx>
    </default>
    <default type="radiobutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <radiowidth>120</radiowidth>
        <radioheight>75</radioheight>
        <align>center</align>
        <aligny>center</aligny>
        <font>Light28</font>
        <label/>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textureradiofocus colordiffuse="ActiveSpinControlColor" flipx="true">common/radio/radio-button-nf.png</textureradiofocus>
        <textureradionofocus colordiffuse="ButtonTextColor">common/radio/radio-button-nf.png</textureradionofocus>
        <textureradioondisabled colordiffuse="aaffffff" flipx="true">common/radio/radio-button-nf.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="ButtonTextColor">common/radio/radio-button-nf.png</textureradiooffdisabled>
        <textoffsetx>50</textoffsetx>
        <radioposx/>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
    </default>
    <default type="spincontrolex">
        <posx>0</posx>
        <posy>0</posy>
        <width>600</width>
        <height>60</height>
        <label/>
        <font>Light28</font>
        <textureup colordiffuse="ButtonTextColor">common/spinctrl/arrow-up-nf.png</textureup>
        <texturedown colordiffuse="ButtonTextColor">common/spinctrl/arrow-down-nf.png</texturedown>
        <textureupdisabled colordiffuse="aaffffff">common/spinctrl/arrow-up-nf.png</textureupdisabled>
        <texturedowndisabled colordiffuse="aaffffff">common/spinctrl/arrow-down-nf.png</texturedowndisabled>
        <textureupfocus colordiffuse="ActiveSpinControlColor">common/spinctrl/arrow-up-nf.png</textureupfocus>
        <texturedownfocus colordiffuse="ActiveSpinControlColor">common/spinctrl/arrow-down-nf.png</texturedownfocus>
        <spinwidth>40</spinwidth>
        <spinheight>60</spinheight>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <width>23</width>
        <height>23</height>
        <textureup colordiffuse="ButtonTextColor">common/spinctrl/arrow-up-nf.png</textureup>
        <texturedown colordiffuse="ButtonTextColor">common/spinctrl/arrow-down-nf.png</texturedown>
        <textureupdisabled colordiffuse="aaffffff">common/spinctrl/arrow-up-nf.png</textureupdisabled>
        <texturedowndisabled colordiffuse="aaffffff">common/spinctrl/arrow-down-nf.png</texturedowndisabled>
        <textureupfocus colordiffuse="ActiveSpinControlColor">common/spinctrl/arrow-up-nf.png</textureupfocus>
        <texturedownfocus colordiffuse="ActiveSpinControlColor">common/spinctrl/arrow-down-nf.png</texturedownfocus>
        <spinwidth>40</spinwidth>
        <spinheight>60</spinheight>
        <font>Light28</font>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <align>center</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
    </default>
    <default type="sliderex">
        <posx>0</posx>
        <posy>0</posy>
        <height>60</height>
        <width>450</width>
        <sliderwidth>220</sliderwidth>
        <sliderheight>50</sliderheight>
        <orientation>horizontal</orientation>
        <orientation>horizontal</orientation>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
        <texturesliderbar border="5" colordiffuse="ButtonTextColor">common/modern/sliderex/slider.png</texturesliderbar>
        <textureslidernib border="5" colordiffuse="ButtonTextColor">common/modern/sliderex/nib.png</textureslidernib>
        <textureslidernibfocus border="5" colordiffuse="ActiveSpinControlColor">common/modern/sliderex/nib.png</textureslidernibfocus>
        <aligny>center</aligny>
        <font>Light28</font>
        <textcolor>ButtonTextColor</textcolor>
        <focusedcolor>ButtonFocusTextColor</focusedcolor>
        <disabledcolor>aaffffff</disabledcolor>
        <textoffsetx>50</textoffsetx>
        <textoffsety>0</textoffsety>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="progress">
        <posx>0</posx>
        <posy>0</posy>
        <width>280</width>
        <height>20</height>
        <info/>
        <texturebg border="5" colordiffuse="OSDPanelColor">diffuse/panel.png</texturebg>
		<lefttexture/>
		<midtexture border="5" colordiffuse="ViewDetailsHighlightTextColor">diffuse/panel.png</midtexture>
        <righttexture/>
        <overlaytexture/>
    </default>
    <default type="slider">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>20</height>
        <orientation>horizontal</orientation>
        <texturefocus colordiffuse="ButtonFocusColor" border="5">diffuse/panel.png</texturefocus>
        <texturenofocus colordiffuse="ButtonColor" border="5">diffuse/panel.png</texturenofocus>
        <texturesliderbar border="5" colordiffuse="ButtonColor">common/modern/slider/slider.png</texturesliderbar>
        <textureslidernib border="5" colordiffuse="ButtonTextColor">common/modern/slider/nib.png</textureslidernib>
        <textureslidernibfocus border="5" colordiffuse="ActiveSpinControlColor">common/modern/slider/nib_focus.png</textureslidernibfocus>
    </default>
    <default type="scrollbar">
        <width>25</width>
        <texturesliderbackground border="5" colordiffuse="ScrollbarBackgroundColor">diffuse/panel.png</texturesliderbackground>
		<texturesliderbar colordiffuse="ScrollbarUnfocusedColor" border="5">diffuse/panel.png</texturesliderbar>
		<texturesliderbarfocus colordiffuse="ScrollbarFocusedColor" border="2">diffuse/panel.png</texturesliderbarfocus>
		<textureslidernib colordiffuse="ScrollbarUnfocusedColor" border="5">diffuse/panel.png</textureslidernib>
		<textureslidernibfocus colordiffuse="ScrollbarFocusedColor" border="5">diffuse/panel.png</textureslidernibfocus>
		<showonepage>false</showonepage>
        <orientation>vertical</orientation>
        <visible>!Skin.HasSetting(DisableScrollbars)</visible>
    </default>
    <default type="textbox">
        <label/>
        <font>Light28</font>
        <textcolor fallback="white">GeneralTextColor</textcolor>
        <shadowcolor/>
        <pagecontrol>61</pagecontrol>
		<align>justify</align>
        <autoscroll delay="8000" time="3000" repeat="10000">!Skin.HasSetting(DisablePlotAutoScroll)</autoscroll>
    </default>
</includes>
